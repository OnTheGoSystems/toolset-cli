{
  "name": "toolset/toolset-cli",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "description": "WP-CLI commands for Toolset plugins",
  "keywords": [
    "wp-cli",
    "toolset",
    "wordpress"
  ],
  "homepage": "https://git.onthegosystems.com/toolset/toolset-cli/",
  "authors": [
    {
      "name": "OnTheGoSystems",
      "email": "hello@onthegosystems.com"
    }
  ],
  "require": {
    "php": ">=5.3"
  },
  "require-dev": {
    "otgs/toolset-coding-standards": "dev-master",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
    "roave/security-advisories": "dev-master",
    "phpcompatibility/php-compatibility": "^9.1",
    "phpcompatibility/phpcompatibility-wp": "^2.0"
  },
  "autoload": {
    "classmap": [
      "application/"
